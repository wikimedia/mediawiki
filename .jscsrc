{
	"preset": "wikimedia",
	"es3": true,

	"requireVarDeclFirst": null,
	"jsDoc": null,

	"disallowQuotedKeysInObjects": "allButReserved",
	"requireDotNotation": { "allExcept": [ "keywords" ] },

	"excludeFiles": [
		"docs/**",
		"extensions/**",
		"node_modules/**",
		"resources/lib/**",
		"resources/src/jquery.tipsy/**",
		"resources/src/jquery/jquery.farbtastic.js",
		"resources/src/mediawiki.libs/**",
		"skins/**",
		"vendor/**"
	]
}
