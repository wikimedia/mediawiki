[project]
	description = MediaWiki core
[access]
	inheritFrom = mediawiki
[access "refs/*"]
	owner = group Administrators
	owner = group mediawiki
	push = deny group Registered Users
	pushTag = +force group mediawiki
	exclusiveGroupPermissions = push pushTag submit
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/for/refs/*"]
	exclusiveGroupPermissions = push
	create = deny group Registered Users
	push = deny group Registered Users
