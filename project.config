[project]
	description = MediaWiki core
[access]
	inheritFrom = mediawiki
[access "refs/*"]
	owner = group Administrators
	create = group mediawiki
	submit = group mediawiki
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/for/refs/heads/*"]
	push = group mediawiki
	pushMerge = group mediawiki
	pushTag = group mediawiki
