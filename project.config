[project]
	description = MediaWiki core
	state = active
[access]
	inheritFrom = mediawiki
[access "refs/*"]
	owner = group Administrators
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/heads/Wikidata"]
	owner = group wikidata
	create = group wikidata
	submit = group wikidata
	forgeCommitter = group wikidata
	push = group wikidata
	pushMerge = group wikidata
	label-Code-Review = -2..+2 group wikidata
	label-Verified = -1..+1 group wikidata
[access "refs/heads/refs/heads/Wikidata"]
	create = group wikidata
	forgeCommitter = group wikidata
	push = group wikidata
	pushMerge = group wikidata
[access "refs/tags/*"]
	pushTag = group mediawiki
