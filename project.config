[project]
	description = MediaWiki core
[access]
	inheritFrom = mediawiki
[access "refs/*"]
	owner = group Administrators
	read = group mediawiki-replication
	push = +force group Administrators
	push = +force group platform-engineering
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
[access "refs/tags/wmf/*"]
	create = block group Registered Users
