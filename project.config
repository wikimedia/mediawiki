[project]
	description = MediaWiki core (phase3)
[access]
	inheritFrom = mediawiki
[access "refs/*"]
	owner = group Administrators
	owner = group mediawiki
	exclusiveGroupPermissions = push pushMerge
	push = +force group platform-engineering
	pushMerge = group platform-engineering
[receive]
	requireChangeId = true
[submit]
	mergeContent = true
